ser.avatarPath) {\n            await deleteObject(ref(storage, user.avatarPath));\n          }\n          const snap = await uploadBytes(imgRef, img);\n          const url = await getDownloadURL(ref(storage, snap.ref.fullPath));\n          await updateDoc(doc(db, \"users\", auth.currentUser.uid), {\n            avatar: url,\n            avatarPath: snap.ref.fullPath,\n          });\n          console.log(url);\n          setImg(\"\");\n        } catch (error) {\n          console.log(error);\n        }\n      };\n      uploadImg();\n    }\n  }, [img]);\n\n\n\n  //color scheme set\n  const backgroundColor = dark ? \"#121212\" : \"#F2EEEE\";\n  const primary = dark ? \"white\" : \"black\";\n  return user ? (\n    <div\n      className=\"profile-wrapper\"\n      style={{ backgroundColor: backgroundColor }}\n    >\n      <div className=\"avatar-wrapper\">\n        <img src={user.avatar || Default} alt=\"avatar\" />\n        <div className=\"overlay\">\n          <label htmlFor=\"photo\">\n            <Camera />\n          </label>\n          <input\n            type=\"file\"\n            accept=\"image/\"\n            style={{ display: \"none\" }}\n            id=\"photo\"\n            onChange={(e) => {\n              setImg(e.target.files[0]);\n              console.log(img);\n            }}\n          />\n        </div>\n      </div>\n      <div className=\"user-detail-wrapper\">\n        <h2>{user.name}</h2>\n        <p>{user.email}</p>\n        <small>Joined at: {user.createdAt.toDate().toDateString()}</small>\n      </div>\n    </div>\n  ): <>error</>;\n};\n\nexport default Profile;\n","/Users/harsh_ju/Documents/Projects/Final Bulds/baatchit/src/components/svgs/Camera.js",[],"/Users/harsh_ju/Documents/Projects/Final Bulds/baatchit/src/components/UserBadge/UserBadge.js",["97","98"],"import React, { useState, useEffect } from \"react\";\nimport \"./UserBadge.css\";\nimport Default from \"../../Assets/basic.jpg\";\n\nconst UserBadge = ({ user, selectUser }) => {\n  c